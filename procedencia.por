programa {
  funcao inicio() {
    real cod

    escreva("Digite o codigo do produto: ")
    leia(cod)

    se (cod == 1){
      escreva("Produto de Procedencia Norte!")
    }
    
    se (cod == 2 ){
      escreva("Produto de Procedencia Nordeste!")
    }

    se (cod == 3){
      escreva("Produto de Procedencia Sul!")
    }

    se (cod == 4){
      escreva("Produto de Procedencia Suldeste")
    }
    
  }
}
