programa {
  funcao inicio() {
    real pacote

    escreva("Digite o pacote escolhido: ")

    escreva("\n1 - Dia das Criança")
   
    escreva("\n2 - Natal")
   
    escreva("\n3 - Ano Novo")
    
    leia(pacote)

    se (pacote == 1){
      escreva("Dia das Crianças, R$500,00")

    }

    se (pacote == 2){
      escreva("Natal, R$700,00")
    }

    se (pacote == 3){
      escreva("Ano Novo, R$1.500,00")
    }


    
    
  }
}
