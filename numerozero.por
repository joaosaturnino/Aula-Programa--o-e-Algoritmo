programa {
  funcao inicio() {
    real teste

    escreva("Digite um numero: ")
    leia(teste)

   


    se (teste < 0 ){
      escreva("Numero menor que zero!")
    }

    se ( teste == 0){
      escreva("Numero igual a zero!")
    }

    se (teste > 0){
      escreva("Numero maior que zero!")
    }
    
  }
}
